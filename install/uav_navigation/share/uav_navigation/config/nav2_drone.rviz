Panels:
  - Class: rviz_common/Displays
    Name: Displays
  - Class: rviz_common/Views
    Name: Views
  - Class: nav2_rviz_plugins/Navigation 2
    Name: Navigation 2

Visualization Manager:
  Class: ""
  Displays:
    # ============== TEMEL ==============
    - Class: rviz_default_plugins/Grid
      Name: Grid
      Enabled: true
      Value: true
      Cell Size: 1
      Color: 160; 160; 164
      Line Style:
        Line Width: 0.03
        Value: Lines
      Normal Cell Count: 0
      Offset:
        X: 0
        Y: 0
        Z: 0
      Plane: XY
      Plane Cell Count: 100
      Reference Frame: <Fixed Frame>

    - Class: rviz_default_plugins/TF
      Name: TF
      Enabled: true
      Value: true
      Marker Scale: 0.3
      Show Arrows: true
      Show Axes: true
      Show Names: true
      Update Interval: 0
      Frame Timeout: 15
      Frames:
        All Enabled: false
        map:
          Value: true
        odom:
          Value: true
        base_link:
          Value: true
        camera_link:
          Value: true

    # ============== ROBOT ==============
    - Class: rviz_default_plugins/Polygon
      Name: Robot Footprint
      Enabled: true
      Value: true
      Topic:
        Value: /local_costmap/published_footprint
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
      Color: 0; 255; 255

    - Class: rviz_default_plugins/Odometry
      Name: Odometry
      Enabled: true
      Value: true
      Angle Tolerance: 0.1
      Covariance:
        Orientation:
          Alpha: 0.5
          Color: 255; 255; 127
          Color Style: Unique
          Frame: Local
          Offset: 1
          Scale: 1
          Value: false
        Position:
          Alpha: 0.3
          Color: 204; 51; 204
          Scale: 1
          Value: false
        Value: false
      Enabled: true
      Keep: 1
      Name: Odometry
      Position Tolerance: 0.1
      Shape:
        Alpha: 1
        Axes Length: 0.8
        Axes Radius: 0.08
        Color: 0; 255; 255
        Head Length: 0.2
        Head Radius: 0.15
        Shaft Length: 0.4
        Shaft Radius: 0.08
        Value: Arrow
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /odom
      Value: true

    # ============== COSTMAPS ==============
    - Class: rviz_default_plugins/Map
      Name: Global Costmap
      Enabled: true
      Value: true
      Alpha: 0.6
      Color Scheme: costmap
      Draw Behind: true
      Topic:
        Value: /global_costmap/costmap
        Depth: 1
        Durability Policy: Transient Local
        History Policy: Keep Last
        Reliability Policy: Reliable
      Update Topic:
        Value: /global_costmap/costmap_updates
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
      Use Timestamp: false

    - Class: rviz_default_plugins/Map
      Name: Local Costmap
      Enabled: true
      Value: true
      Alpha: 0.8
      Color Scheme: costmap
      Draw Behind: false
      Topic:
        Value: /local_costmap/costmap
        Depth: 1
        Durability Policy: Transient Local
        History Policy: Keep Last
        Reliability Policy: Reliable
      Update Topic:
        Value: /local_costmap/costmap_updates
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
      Use Timestamp: false

    # ============== OCTOMAP 2D PROJEKSİYON (Nav2 için) ==============
    - Class: rviz_default_plugins/Map
      Name: Octomap 2D Projection
      Enabled: true
      Value: true
      Alpha: 0.5
      Color Scheme: map
      Draw Behind: true
      Topic:
        Value: /map
        Depth: 1
        Durability Policy: Transient Local
        History Policy: Keep Last
        Reliability Policy: Reliable
      Use Timestamp: false

    # ============== OCTOMAP 3D VOXEL GÖRSELLEŞTİRME ==============
    # Dolu hücreler (engeller) - YEŞİL/MAVİ yüksekliğe göre
    - Class: rviz_default_plugins/MarkerArray
      Name: Octomap 3D Occupied
      Enabled: true
      Value: true
      Topic:
        Value: /occupied_cells_vis_array
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable

    # Boş hücreler (keşfedilmiş boş alan) - Şeffaf
    - Class: rviz_default_plugins/MarkerArray
      Name: Octomap 3D Free
      Enabled: false
      Value: false
      Topic:
        Value: /free_cells_vis_array
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable

    # Voxel merkezleri (PointCloud olarak)
    - Class: rviz_default_plugins/PointCloud2
      Name: Octomap Point Centers
      Enabled: false
      Value: false
      Alpha: 1.0
      Autocompute Intensity Bounds: true
      Autocompute Value Bounds:
        Max Value: 5
        Min Value: 0
        Value: true
      Axis: Z
      Channel Name: z
      Color: 255; 100; 0
      Color Transformer: AxisColor
      Decay Time: 0
      Invert Rainbow: false
      Max Color: 255; 0; 0
      Min Color: 0; 0; 255
      Position Transformer: XYZ
      Selectable: true
      Size (Pixels): 3
      Size (m): 0.15
      Style: Boxes
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /octomap_point_cloud_centers
      Use Fixed Frame: true
      Use rainbow: true

    # ============== LOCAL COSTMAP VOXEL ==============
    - Class: rviz_default_plugins/PointCloud2
      Name: Local Voxel Grid
      Enabled: true
      Value: true
      Alpha: 0.8
      Autocompute Intensity Bounds: true
      Autocompute Value Bounds:
        Max Value: 10
        Min Value: -10
        Value: true
      Axis: Z
      Channel Name: intensity
      Color: 255; 0; 0
      Color Transformer: FlatColor
      Decay Time: 0
      Enabled: true
      Invert Rainbow: false
      Max Color: 255; 255; 255
      Max Intensity: 4096
      Min Color: 0; 0; 0
      Min Intensity: 0
      Name: Local Voxel Grid
      Position Transformer: XYZ
      Selectable: true
      Size (Pixels): 3
      Size (m): 0.1
      Style: Boxes
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /local_costmap/voxel_marked_cloud
      Use Fixed Frame: true
      Use rainbow: false
      Value: true

    # ============== PATH PLANNING ==============
    - Class: rviz_default_plugins/Path
      Name: Global Path
      Enabled: true
      Value: true
      Alpha: 1
      Buffer Length: 1
      Color: 0; 255; 0
      Head Diameter: 0.1
      Head Length: 0.05
      Length: 0.3
      Line Style: Billboards
      Line Width: 0.08
      Offset:
        X: 0
        Y: 0
        Z: 0.1
      Pose Color: 255; 85; 255
      Pose Style: None
      Radius: 0.03
      Shaft Diameter: 0.05
      Shaft Length: 0.1
      Topic:
        Value: /plan
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable

    - Class: rviz_default_plugins/Path
      Name: Local Path
      Enabled: true
      Value: true
      Alpha: 1
      Buffer Length: 1
      Color: 255; 255; 0
      Head Diameter: 0.05
      Head Length: 0.025
      Length: 0.15
      Line Style: Billboards
      Line Width: 0.05
      Offset:
        X: 0
        Y: 0
        Z: 0.15
      Pose Color: 255; 85; 255
      Pose Style: None
      Radius: 0.02
      Shaft Diameter: 0.025
      Shaft Length: 0.05
      Topic:
        Value: /local_plan
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable

    # ============== SENSÖRLER ==============
    - Class: rviz_default_plugins/PointCloud2
      Name: Depth Camera Points
      Enabled: true
      Value: true
      Alpha: 0.5
      Autocompute Intensity Bounds: true
      Autocompute Value Bounds:
        Max Value: 10
        Min Value: -10
        Value: true
      Axis: Z
      Channel Name: intensity
      Color: 255; 255; 255
      Color Transformer: AxisColor
      Decay Time: 0
      Enabled: true
      Invert Rainbow: false
      Max Color: 255; 255; 255
      Max Intensity: 4096
      Min Color: 0; 0; 0
      Min Intensity: 0
      Name: Depth Camera Points
      Position Transformer: XYZ
      Selectable: true
      Size (Pixels): 2
      Size (m): 0.02
      Style: Points
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Best Effort
        Value: /camera/points
      Use Fixed Frame: true
      Use rainbow: true
      Value: true

    # ============== VELOCITY COMMAND ==============
    - Class: rviz_default_plugins/Marker
      Name: Velocity Arrow
      Enabled: true
      Value: true
      Topic:
        Value: /velocity_marker
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable

    # ============== GOAL MARKER ==============
    - Class: rviz_default_plugins/Pose
      Name: Goal Pose
      Enabled: true
      Value: true
      Alpha: 1
      Axes Length: 1
      Axes Radius: 0.1
      Color: 255; 0; 255
      Head Length: 0.3
      Head Radius: 0.2
      Shaft Length: 0.5
      Shaft Radius: 0.1
      Shape: Arrow
      Topic:
        Value: /goal_pose
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable

  Global Options:
    Background Color: 30; 30; 30
    Fixed Frame: map
    Frame Rate: 30

  Tools:
    - Class: rviz_default_plugins/Interact
      Hide Inactive Objects: true
    - Class: rviz_default_plugins/MoveCamera
    - Class: rviz_default_plugins/Select
    - Class: rviz_default_plugins/FocusCamera
    - Class: rviz_default_plugins/Measure
      Line color: 128; 128; 0
    - Class: rviz_default_plugins/SetInitialPose
      Topic:
        Value: /initialpose
    - Class: rviz_default_plugins/SetGoal
      Topic:
        Value: /goal_pose
    - Class: rviz_default_plugins/PublishPoint
      Single click: true
      Topic:
        Value: /clicked_point

  Views:
    Current:
      Class: rviz_default_plugins/ThirdPersonFollower
      Distance: 12
      Enable Stereo Rendering:
        Stereo Eye Separation: 0.06
        Stereo Focal Distance: 1
        Swap Stereo Eyes: false
        Value: false
      Focal Point:
        X: 0
        Y: 0
        Z: 0
      Focal Shape Fixed Size: true
      Focal Shape Size: 0.05
      Invert Z Axis: false
      Name: Current View
      Near Clip Distance: 0.01
      Pitch: 0.6
      Target Frame: base_link
      Value: ThirdPersonFollower
      Yaw: 3.14
    Saved:
      - Class: rviz_default_plugins/TopDownOrtho
        Name: Top Down
        Enable Stereo Rendering:
          Value: false
        Invert Z Axis: false
        Near Clip Distance: 0.01
        Scale: 20
        Target Frame: base_link
        X: 0
        Y: 0

Window Geometry:
  Displays:
    collapsed: false
  Height: 1016
  Hide Left Dock: false
  Hide Right Dock: false
  Navigation 2:
    collapsed: false
  QMainWindow State: 
  Views:
    collapsed: false
  Width: 1920
  X: 0
  Y: 0